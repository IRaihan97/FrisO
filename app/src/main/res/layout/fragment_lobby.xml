<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/lobbyBar2"
        android:layout_width="381dp"
        android:layout_height="66dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.163"
        app:srcCompat="?attr/colorButtonNormal" />

    <TextView
        android:id="@+id/lobbyText2"
        android:layout_width="247dp"
        android:layout_height="28dp"
        android:text="TextView"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/lobbyBar2"
        app:layout_constraintEnd_toEndOf="@+id/lobbyBar2"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toStartOf="@+id/lobbyBar2"
        app:layout_constraintTop_toTopOf="@+id/lobbyBar2"
        app:layout_constraintVertical_bias="0.571" />

    <Button
        android:id="@+id/lobbyButton2"
        android:layout_width="102dp"
        android:layout_height="54dp"
        android:text="Join"
        app:layout_constraintBottom_toBottomOf="@+id/lobbyBar2"
        app:layout_constraintEnd_toEndOf="@+id/lobbyBar2"
        app:layout_constraintHorizontal_bias="0.955"
        app:layout_constraintStart_toStartOf="@+id/lobbyBar2"
        app:layout_constraintTop_toTopOf="@+id/lobbyBar2" />

    <ImageView
        android:id="@+id/lobbyBar1"
        android:layout_width="381dp"
        android:layout_height="66dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024"
        app:srcCompat="?attr/colorButtonNormal" />

    <TextView
        android:id="@+id/lobbyText1"
        android:layout_width="247dp"
        android:layout_height="26dp"
        android:text="TextView"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/lobbyBar1"
        app:layout_constraintEnd_toEndOf="@+id/lobbyBar1"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toStartOf="@+id/lobbyBar1"
        app:layout_constraintTop_toTopOf="@+id/lobbyBar1"
        app:layout_constraintVertical_bias="0.57" />

    <Button
        android:id="@+id/lobbyButton1"
        android:layout_width="102dp"
        android:layout_height="54dp"
        android:text="Join"
        app:layout_constraintBottom_toBottomOf="@+id/lobbyBar1"
        app:layout_constraintEnd_toEndOf="@+id/lobbyBar1"
        app:layout_constraintHorizontal_bias="0.955"
        app:layout_constraintStart_toStartOf="@+id/lobbyBar1"
        app:layout_constraintTop_toTopOf="@+id/lobbyBar1" />
</androidx.constraintlayout.widget.ConstraintLayout>